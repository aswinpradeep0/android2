<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="main">
        <item name="android:textColor">#0f68ac</item>
        <item name="android:textSize">35dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#ffffff</item>
    </style>
    <style name="lt"  >
      <!-- <item name="android:background">@drawable/imageets</item>-->
        <item name="android:padding">30dp</item>
        <item name="android:columnCount">2</item>
        <item name="android:rowCount">2</item>
    </style>
    <style name="ltsec" parent="lt">
        <item name="android:background">#E0D1D1</item>
        <item name="android:padding">30dp</item>
        <item name="android:columnCount">2</item>
        <item name="android:rowCount">2</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="intext">
        <item name="android:textColor">#ff66d9	</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textSize">25dp</item>
    </style>
    <style name="intext1">
        <item name="android:textColor">#feffff</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textSize">15dp</item>

    </style>
    <style name="btn" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">@android:color/holo_blue_dark</item>
    </style>

    <style name="txtsize">
        <item name="android:textSize">50dp</item>
    </style>

    <style name="btn2" parent="txtsize">
        <item name="android:textColor">#ff0000</item>
    </style>


</resources>